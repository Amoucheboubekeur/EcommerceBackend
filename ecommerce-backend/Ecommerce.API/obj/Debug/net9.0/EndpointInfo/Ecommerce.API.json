      "post": {
        "tags": [
          "Products"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "CategoryId",
                  "Price",
                  "Stock",
                  "Title"
                ],
                "type": "object",
                "properties": {
                  "Title": {
                    "maxLength": 200,
                    "minLength": 0,
                    "type": "string"
                  },
                  "Description": {
                    "maxLength": 2000,
                    "minLength": 0,
                    "type": "string"
                  },
                  "Price": {
                    "minimum": 0,01,
                    "type": "number",
                    "format": "double"
                  },
                  "Stock": {
                    "maximum": 2147483647,
                    "minimum": 0,
                    "type": "integer",
                    "format": "int32"
                  },
                  "CategoryId": {
                    "type": "string",
                    "format": "uuid"
                  },
                  "DiscountPercentage": {
                    "maximum": 100,
                    "minimum": 0,
                    "type": "number",
                    "format": "double"
                  },
                  "DiscountStartDate": {
                    "type": "string",
                    "format": "date-time"
                  },
                  "DiscountEndDate": {
                    "type": "string",
                    "format": "date-time"
                  },
                  "DeliveryPriceMaison": {
                    "minimum": 0,
                    "type": "number",
                    "format": "double"
                  },
                  "DeliveryPriceBureau": {
                    "minimum": 0,
                    "type": "number",
                    "format": "double"
                  },
                  "Image": {
                    "type": "string",
                    "format": "binary"
                  },
                  "Images": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "format": "binary"
                    }
                  },
                  "Variants": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/CreateProductVariantDTO"
                    }
                  }
                }
              },
              "encoding": {
                "Title": {
                  "style": "form"
                },
                "Description": {
                  "style": "form"
                },
                "Price": {
                  "style": "form"
                },
                "Stock": {
